#include <iostream>
using namespace std;

class rational{
public:
    
    int add;
    int sub;
    int div;
    bool le;
    int num;
    int den;
    
    rational (){
        result;
    }
    rational(int a, int b){
        num=a;
        den=b;

    }
    
    
    rational addition (rational x)
    {
      result.num=x.num+x.den;
      return result;
    };
    
    rational subtraction (rational x)
    {
        rational result;
        rational.num=x.num-x.den;
        return rational;
    }
    
    rational division (rational x)
    {
        result.num=x.num/x.den;
        return result;
    }
    
    rational less (rational x)
    {
        result.num=x.num/x.den;
        return result;
    }
    
};


int main() {
    
    rational a(6, 5);
    rational b(3, 4);

    
    rational tmp =  a.addition(b);
    cout << endl;
    rational sub = a.subtraction(b);
    cout << endl;
    rational divs = a.division(b);
    




    return 0;
}
