#include <iostream>

using namespace std;

void f1() {
    
    cout << "Selected";
}

void f2(){
    cout << "Selected";
}

void f3(){
    cout << "Selected";
}


int main() {
    int x;
    cout <<"Main menu \n"; 
    cout << "Select" << "\n";
    cin >> x;
    if (x = 1) {
        f1();
    }
    
    if (x = 2) {
        f2();
    }

    if (x = 3) {
        f3();
        
    }

    return 0;
}
