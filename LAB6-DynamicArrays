#include <iostream>
#include <fstream>
using namespace std;

int main() {
    
    const int ROWS = 4;
    int COLUMNS_PER_ROW_ARR [ROWS] = {5, 6, 4, 3};
    int **arr = new int *[ROWS];
    arr[0] = new int [5];
    arr[1] = new int [6];
    arr[2] = new int [4];
    arr[3] = new int [3];
    
    int userID = 49193;
    int station = 2;
    int lab = 3;
    
    //cout << "User ID: ";
    //cin >> iserID;
    //cout << endl;
    //cout << "Station: ";
    //cin >> station;
    //cout << endl;
    //cout << "Lab: ";
    //cin >> lab;
    cout << endl;
    
    arr[lab - 1][station - 1] = userID;
    cout << "Lab Number Computer Stations \n";
    
    for (int i = 0; i < ROWS; i++){
        for (int j = 0; j < COLUMNS_PER_ROW_ARR[i]; j++){
            cout << arr[i][j] << " ";
            
            if (arr[i][j] == 0){
            cout << 0;
            }
           
        }
        cout << endl;
    }
    
    cout << endl;
    return 0;
}
