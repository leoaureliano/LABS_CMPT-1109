#include <iostream>
using namespace std;

int fact(int n){

    if(n == 1 || n == 0)
        return 1; //Basic stap
    else
        return n * (fact(n-1));
    

}
    
int main() {
    
    cout << fact(0) << endl;
    cout << fact(1) << endl;
    cout << fact(2) << endl;
    cout << fact(3) << endl;
    cout << fact(4) << endl;
    
    return 0;
}
